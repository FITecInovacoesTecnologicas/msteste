{
  "timestamp": {
    "$resolver": "timestamp",
    "pattern": {
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
      "timeZone": "UTC"
    }
  },
  "application": "[Messaging-Service]",
  "level": { 
	"$resolver": "level",
	"field": "name"
  },
  "message": { 
	"$resolver": "message", 
	"stringified": true 
  },
  "exception": {
    "$resolver": "exception",
    "field": "stackTrace",
    "stackTrace": {
      "stringified": true
    }
  }
}
